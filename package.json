{
  "name": "@neogeek/nodejs-starter-kit",
  "version": "0.0.1",
  "description": "This repo is a starter kit for working with Node.js, Handlebars (both server and client side), SASS (through node-sass), and RequireJS.",
  "engines": {
    "node": "0.12.x"
  },
  "main": "web.js",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.13.3",
    "bower": "1.4.1",
    "compression": "1.5.2",
    "express": "4.13.3",
    "express-enrouten": "1.2.1",
    "express-hbs": "0.8.4",
    "express-session": "1.11.3",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-handlebars": "0.10.2",
    "grunt-contrib-jshint": "0.11.2",
    "grunt-sass": "1.0.0",
    "load-grunt-tasks": "3.2.0"
  },
  "devDependencies": {
    "grunt-contrib-watch": "0.6.1",
    "mocha": "2.2.5"
  },
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install && ./node_modules/grunt-cli/bin/grunt default",
    "test": "make test"
  },
  "keywords": [
    "node",
    "bower",
    "express",
    "grunt",
    "handlebars",
    "jasmine",
    "sass",
    "jshint"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/neogeek/nodejs-starter-kit.git"
  },
  "private": true
}
