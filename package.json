{
  "name": "@neogeek/nodejs-starter-kit",
  "version": "0.0.1",
  "description": "This repo is a starter kit for working with Node.js, Handlebars (both server and client side), SASS (through node-sass), and RequireJS.",
  "engines": {
    "node": "5.x"
  },
  "main": "web.js",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.15.0",
    "bower": "1.7.7",
    "compression": "1.6.1",
    "cors": "2.7.1",
    "express": "4.13.4",
    "express-enrouten": "1.3.0",
    "express-hbs": "0.8.4",
    "express-session": "1.13.0",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-handlebars": "1.0.0",
    "grunt-contrib-jshint": "1.0.0",
    "grunt-sass": "1.1.0",
    "i18n": "0.8.1",
    "load-grunt-tasks": "3.4.1",
    "requirejs": "2.1.22"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "grunt-contrib-watch": "0.6.1",
    "jscover": "1.0.0",
    "mocha": "2.4.5",
    "mocha-reporter-cov-summary": "0.1.1"
  },
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install && ./node_modules/grunt-cli/bin/grunt default && make requirejs",
    "start": "node web.js",
    "test": "make test"
  },
  "keywords": [
    "node",
    "bower",
    "express",
    "grunt",
    "handlebars",
    "jasmine",
    "sass",
    "jshint"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/neogeek/nodejs-starter-kit.git"
  },
  "private": true
}
